	<definition name="*/*/*.front_empty" extends="layout_empty_front">
		<put-attribute name="content" value="/WEB-INF/jsp/{1}/{2}/{3}.jsp" />
	</definition>

	<definition name="*/*.common_empty" extends="layout_empty_front">
		<put-attribute name="content" value="/WEB-INF/jsp/{1}/{2}.jsp" />
	</definition>



</tiles-definitions>
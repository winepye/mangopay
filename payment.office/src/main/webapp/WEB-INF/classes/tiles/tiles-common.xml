		<put-attribute name="content" value="/WEB-INF/jsp/admin/{1}/{2}.jsp" />
	</definition>

	<definition name="admin/*/*.admin_empty" extends="layout_empty_admin">
		<put-attribute name="content" value="/WEB-INF/jsp/admin/{1}/{2}.jsp" />
